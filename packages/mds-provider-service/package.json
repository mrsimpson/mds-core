{
  "name": "@mds-core/mds-provider-service",
  "version": "0.2.0",
  "description": "Provider Service",
  "keywords": [
    "mds",
    "mds-providers"
  ],
  "license": "Apache-2.0",
  "author": "Lacuna Technologies",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "build": "pnpm _build",
    "start": "pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "pnpm build && ts-node -r ../../dotenv-flow.js",
    "typeorm": "typeorm-ts-node-commonjs -r ../../dotenv-flow.js -d ormconfig",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-repository": "workspace:0.2.3",
    "@mds-core/mds-rpc-common": "workspace:0.3.22",
    "@mds-core/mds-schema-validators": "workspace:0.5.12",
    "@mds-core/mds-service-helpers": "workspace:0.6.10",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "pg": "8.7.3",
    "typeorm": "0.3.4"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
