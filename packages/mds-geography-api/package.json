    "@mds-core/mds-schema-validators": "workspace:0.5.10",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.0",
    "@types/express": "4.17.13",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.28"
  },
  "publishConfig": {
    "main": "dist/index.js"
