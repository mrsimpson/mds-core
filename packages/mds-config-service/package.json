{
  "name": "@mds-core/mds-config-service",
  "version": "0.4.12",
  "description": "Mobility Data Specification configuration service",
  "keywords": [
    "mds",
    "config"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "jest",
    "build": "pnpm _build",
    "start": "pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "node --experimental-repl-await -r ts-node/register -r ../../dotenv-flow.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-config-files": "workspace:0.1.6",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-rpc-common": "workspace:0.3.22",
    "@mds-core/mds-service-helpers": "workspace:0.6.10",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "json5": "2.2.0"
  }
}
