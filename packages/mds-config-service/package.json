{
  "name": "@mds-core/mds-config-service",
  "version": "0.4.8",
  "description": "Mobility Data Specification configuration service",
  "keywords": [
    "mds",
    "config"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "test": "DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-config-files": "workspace:0.1.4",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-rpc-common": "workspace:0.3.18",
    "@mds-core/mds-service-helpers": "workspace:0.6.8",
    "@mds-core/mds-utils": "workspace:0.5.0",
    "json5": "2.2.0"
  }
}
