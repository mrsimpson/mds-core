{
  "name": "@mds-core/mds-ingest-migration-processor",
  "version": "0.0.51",
  "description": "MDS Ingest Data Migration Processor",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch index.ts",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand --passWithNoTests",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-agency-cache": "workspace:0.4.17",
    "@mds-core/mds-api-server": "workspace:0.4.6",
    "@mds-core/mds-ingest-service": "workspace:0.9.15",
    "@mds-core/mds-logger": "workspace:0.5.6",
    "@mds-core/mds-repository": "workspace:0.1.41",
    "@mds-core/mds-service-helpers": "workspace:0.6.4",
    "@mds-core/mds-stream-processor": "workspace:0.4.12",
    "@mds-core/mds-types": "workspace:0.9.0",
    "@mds-core/mds-utils": "workspace:0.4.0",
    "@types/express": "4.17.12",
    "envalid": "7.2.1",
    "express": "4.17.1"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
