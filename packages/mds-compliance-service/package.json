{
  "name": "@mds-core/mds-compliance-service",
  "version": "0.10.31",
  "description": "Compliance Service",
  "keywords": [
    "mds",
    "compliance"
  ],
  "license": "UNLICENSED",
  "author": "Lacuna Technologies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "build": "pnpm _build",
    "start": "pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-db": "workspace:0.6.17",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-policy-service": "workspace:0.11.28",
    "@mds-core/mds-providers": "workspace:0.2.11",
    "@mds-core/mds-repository": "workspace:0.2.0",
    "@mds-core/mds-rpc-common": "workspace:0.3.19",
    "@mds-core/mds-schema-validators": "workspace:0.5.10",
    "@mds-core/mds-service-helpers": "workspace:0.6.8",
    "@mds-core/mds-stream": "workspace:0.3.1",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.0",
    "pg": "8.7.3",
    "typeorm": "0.3.4"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
