{
  "name": "@mds-core/mds-compliance-service",
  "version": "0.10.18",
  "description": "Compliance Service",
  "keywords": [
    "mds",
    "compliance"
  ],
  "license": "UNLICENSED",
  "author": "Lacuna Technologies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-db": "workspace:0.6.4",
    "@mds-core/mds-logger": "workspace:0.5.6",
    "@mds-core/mds-policy-service": "workspace:0.11.16",
    "@mds-core/mds-providers": "workspace:0.2.7",
    "@mds-core/mds-repository": "workspace:0.1.41",
    "@mds-core/mds-rpc-common": "workspace:0.3.11",
    "@mds-core/mds-schema-validators": "workspace:0.5.4",
    "@mds-core/mds-service-helpers": "workspace:0.6.4",
    "@mds-core/mds-stream": "workspace:0.2.9",
    "@mds-core/mds-types": "workspace:0.9.0",
    "@mds-core/mds-utils": "workspace:0.4.0",
    "pg": "8.7.3",
    "typeorm": "0.2.43"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
