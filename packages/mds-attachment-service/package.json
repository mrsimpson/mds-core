    "@mds-core/mds-service-helpers": "workspace:0.6.10",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "@types/sharp": "0.29.2",
    "aws-sdk": "2.999.0",
    "express": "4.17.1",
    "multer": "1.4.3",
    "pg": "8.7.3",
    "sharp": "0.29.3",
    "typeorm": "0.3.4"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
