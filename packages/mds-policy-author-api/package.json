{
  "name": "@mds-core/mds-policy-author-api",
  "version": "0.7.22",
  "description": "MDS Policy Author API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/policy-author pnpm watch server",
    "test": "PATH_PREFIX=/policy-author DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.13",
    "@mds-core/mds-api-server": "workspace:0.4.7",
    "@mds-core/mds-logger": "workspace:0.5.6",
    "@mds-core/mds-policy-service": "workspace:0.11.17",
    "@mds-core/mds-providers": "workspace:0.2.8",
    "@mds-core/mds-schema-validators": "workspace:0.5.5",
    "@mds-core/mds-service-helpers": "workspace:0.6.5",
    "@mds-core/mds-types": "workspace:0.9.1",
    "@mds-core/mds-utils": "workspace:0.4.1",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-geography-service": "workspace:0.10.11",
    "@mds-core/mds-stream": "workspace:0.2.10",
    "@mds-core/mds-test-data": "workspace:0.4.18"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
