{
  "name": "@mds-core/mds-config-api",
  "version": "0.3.0",
  "description": "Mobility Data Specification configuration API",
  "keywords": [
    "mds",
    "config"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "PATH_PREFIX=/config pnpm watch server",
    "test": "PATH_PREFIX=/config DOTENV_CONFIG_PATH=../../.env jest",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-server": "workspace:0.4.7",
    "@mds-core/mds-config-service": "workspace:0.3.0",
    "@mds-core/mds-logger": "workspace:0.5.6",
    "@mds-core/mds-service-helpers": "workspace:0.6.5",
    "@mds-core/mds-utils": "workspace:0.4.1",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "query-string": "7.0.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "0.4.18",
    "http-status-codes": "2.1.4"
  }
}
