{
  "name": "@mds-core/mds-config-api",
  "version": "0.4.14",
  "description": "Mobility Data Specification configuration API",
  "keywords": [
    "mds",
    "config"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "PATH_PREFIX=/config jest",
    "build": "pnpm _build",
    "start": "PATH_PREFIX=/config pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "node --experimental-repl-await -r ts-node/register -r ../../dotenv-flow.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-server": "workspace:0.4.15",
    "@mds-core/mds-config-service": "workspace:0.4.14",
    "@mds-core/mds-logger": "workspace:0.5.8",
    "@mds-core/mds-service-helpers": "workspace:0.6.11",
    "@mds-core/mds-utils": "workspace:0.5.3",
    "@types/express": "4.17.13",
    "express": "4.18.1",
    "query-string": "7.1.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "0.4.39",
    "http-status-codes": "2.2.0"
  }
}
