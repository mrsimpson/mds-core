{
  "name": "@mds-core/mds-audit-service",
  "version": "0.6.1",
  "description": "Mobility Data Specification Audit Service",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start": "pnpm watch server",
    "pretest": "env $(cat ../../.env | xargs) pnpm typeorm -- schema:drop",
    "test": "DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "DOTENV_CONFIG_PATH=../../.env node --experimental-repl-await -r ts-node/register -r dotenv/config",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-repository": "workspace:0.1.46",
    "@mds-core/mds-rpc-common": "workspace:0.3.17",
    "@mds-core/mds-schema-validators": "workspace:0.5.9",
    "@mds-core/mds-service-helpers": "workspace:0.6.7",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.0",
    "pg": "8.7.3",
    "typeorm": "0.2.43"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}