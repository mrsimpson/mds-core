{
  "name": "@mds-core/mds-stream-processor",
  "version": "0.5.1",
  "description": "MDS Stream Processor",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "jest --runInBand --passWithNoTests",
    "build": "pnpm _build",
    "process": "pnpm watch",
    "start": "pnpm watch",
    "test": "pnpm _test",
    "ts-node": "node --experimental-repl-await -r ts-node/register -r ../../dotenv-flow.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-server": "0.4.14",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-service-helpers": "workspace:0.6.10",
    "@mds-core/mds-stream": "workspace:0.4.1",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "@types/event-stream": "4.0.0",
    "event-stream": "4.0.1"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
