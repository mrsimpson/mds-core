    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.0",
    "@types/event-stream": "3.3.34",
    "event-stream": "4.0.1"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}