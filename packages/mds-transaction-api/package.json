{
  "name": "@mds-core/mds-transaction-api",
  "version": "0.2.38",
  "description": "MDS Transaction API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "PATH_PREFIX=/transaction jest --runInBand",
    "build": "pnpm _build",
    "generate": "pnpm generate:schema && pnpm generate:flatspec",
    "generate:flatspec": "ts-node ./generate-flat-spec.ts && git add spec/flat-spec.json",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/transaction pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "node -r ts-node/register -r ../../dotenv-flow.js",
    "watch": "pnpm build && nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.29",
    "@mds-core/mds-api-server": "workspace:0.4.13",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-providers": "workspace:0.2.11",
    "@mds-core/mds-schema-validators": "workspace:0.5.12",
    "@mds-core/mds-service-helpers": "workspace:0.6.10",
    "@mds-core/mds-transaction-service": "workspace:0.6.29",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "@types/express": "4.17.13",
    "@types/json2csv": "5.0.3",
    "express": "4.17.1",
    "http-status-codes": "2.1.4",
    "json2csv": "5.0.6",
    "luxon": "2.3.0",
    "typeorm-cursor-pagination": "0.8.1"
  },
  "devDependencies": {
    "@mds-core/mds-test-data": "workspace:0.4.32"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
