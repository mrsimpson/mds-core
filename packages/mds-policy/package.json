{
  "name": "@mds-core/mds-policy",
  "version": "0.8.4",
  "description": "MDS Policy API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "_build": "tsc --build tsconfig.build.json",
    "_test": "PATH_PREFIX=/policy jest --runInBand",
    "build": "pnpm _build",
    "generate": "pnpm generate:schema",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/policy pnpm watch server",
    "test": "pnpm _test",
    "ts-node": "pnpm build && ts-node -r ../../dotenv-flow.js",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.30",
    "@mds-core/mds-api-server": "workspace:0.4.13",
    "@mds-core/mds-db": "workspace:0.6.22",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-policy-service": "workspace:0.12.4",
    "@mds-core/mds-schema-validators": "workspace:0.5.13",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.5.2",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-geography-service": "workspace:0.10.27",
    "@mds-core/mds-stream": "workspace:0.3.3",
    "@mds-core/mds-test-data": "workspace:0.4.33"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
