{
  "name": "@mds-core/mds-policy",
  "version": "0.7.28",
  "description": "MDS Policy API",
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "generate": "pnpm generate:schema",
    "generate:schema": "ts-node ./generate-schemas.ts && git add schema-gen",
    "start": "PATH_PREFIX=/policy pnpm watch server",
    "test": "PATH_PREFIX=/policy DOTENV_CONFIG_PATH=../../.env jest --runInBand",
    "ts-node": "pnpm build && DOTENV_CONFIG_PATH=../../.env ts-node -r dotenv/config",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec pnpm ts-node --"
  },
  "dependencies": {
    "@mds-core/mds-api-helpers": "workspace:0.2.19",
    "@mds-core/mds-api-server": "workspace:0.4.8",
    "@mds-core/mds-db": "workspace:0.6.11",
    "@mds-core/mds-logger": "workspace:0.5.7",
    "@mds-core/mds-policy-service": "workspace:0.11.22",
    "@mds-core/mds-schema-validators": "workspace:0.5.7",
    "@mds-core/mds-types": "workspace:0.9.2",
    "@mds-core/mds-utils": "workspace:0.4.2",
    "@types/express": "4.17.13",
    "express": "4.17.1",
    "http-status-codes": "2.1.4"
  },
  "devDependencies": {
    "@mds-core/mds-geography-service": "workspace:0.10.16",
    "@mds-core/mds-stream": "workspace:0.3.0",
    "@mds-core/mds-test-data": "workspace:0.4.22"
  },
  "publishConfig": {
    "main": "dist/index.js"
  }
}
