{
  "name": "@container-images/mds-audit-service",
  "version": "0.1.0",
  "private": true,
  "description": "Docker Image for MDS Audit Service",
  "keywords": [
    "mds"
  ],
  "license": "Apache-2.0",
  "author": "City of Los Angeles",
  "files": [
    "dist/"
  ],
  "scripts": {
    "bundle": "pnpm bundle:production",
    "bundle:development": "webpack --mode=development --env npm_package_name=${npm_package_name} --env npm_package_version=${npm_package_version}",
    "bundle:production": "webpack --mode=production --env npm_package_name=${npm_package_name} --env npm_package_version=${npm_package_version}",
    "image": "pnpm image:production",
    "image:development": "pnpm bundle:development && ../../bin/build-helper buildImage mds-audit-service:latest",
    "image:production": "pnpm bundle:production && ../../bin/build-helper buildImage mds-audit-service:${npm_package_version}"
  },
  "dependencies": {
    "@mds-core/mds-audit-service": "workspace:0.6.2",
    "@mds-core/mds-webpack-config": "workspace:0.1.12",
    "dd-trace": "1.5.1",
    "express": "4.17.1",
    "pg": "8.7.3",
    "pino": "6.13.3"
  }
}
